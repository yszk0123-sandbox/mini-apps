{
  "version": 2,
  "name": "mini-apps",
  "alias": ["mini-apps.yszk0123.site"],
  "env": {
    "APP_PROJECT_ID": "mini-apps-222208",
    "APP_CLIENT_EMAIL": "@app-client-email",
    "APP_PRIVATE_KEY": "@app-private-key",
    "APP_ACCESS_TOKEN": "@app-access-token"
  },
  "builds": [
    { "src": "packages/html/*.html", "use": "@now/static" },
    { "src": "packages/next/package.json", "use": "@now/next" },
    { "src": "packages/counter-api/index.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/html/(.*)", "dest": "/packages/html/$1.html" },
    { "src": "/counter", "dest": "/packages/counter-api/index.js" },
    { "src": "/(.*)", "dest": "/packages/next/$1" }
  ]
}
